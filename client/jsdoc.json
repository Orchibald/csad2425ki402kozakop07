{
  "source": {
    "include": ["src", "electron"],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(node_modules|dist|deploy)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/minami",
    "readme": "./README.md"
  }
}